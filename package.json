  "description": "",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.670.0",
    "@neondatabase/serverless": "^0.10.1",
    "amazon-cognito-identity-js": "^6.3.12",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.2",
    "express": "^4.21.0",
    "express-json": "^1.0.0",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "pg": "^8.13.0",
    "sqs-consumer": "^11.1.0",
    "trim-request": "^1.0.6",
