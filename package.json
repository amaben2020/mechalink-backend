    "@firebase/auth": "^1.8.0",
    "@neondatabase/serverless": "^0.10.1",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-amplify": "^6.6.7",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.2",
    "express": "^4.21.0",
