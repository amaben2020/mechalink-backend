{
  "name": "ec2",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.js",
    "db:push": "npx drizzle-kit push",
    "db:generate": "npx drizzle-kit generate && drizzle-kit generate --dialect=postgresql --schema=src/schema.js --out=./drizzle",
    "db:migrate": "npx drizzle-kit migrate && node ./src/migrate.js",
    "db:studio": "npx drizzle-kit studio"
  },
